    "import yfinance as yf\n",
    "import pandas as pd\n",
    "usd_kzt = yf.Ticker(\"BZ=F\")\n",
    "hist = usd_kzt.history(start='2008-03-26' ,period=\"max\", interval='1d')\n",
    "hist = hist.drop(['Open', 'High', 'Low', 'Volume', 'Dividends', 'Stock Splits'], axis=1)\n",
    "\n",
    "print(len(hist))\n",
    "\n",
    "hist.to_csv('br.csv')"
   ]
  }
